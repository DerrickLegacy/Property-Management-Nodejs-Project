<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <title>Global Masters Uganda</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="Glober Masters Property Managers" name="keywords">
    <meta content="Official Global Masters Page" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet"> 

    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="css/style.css" rel="stylesheet">
</head>

<body>
    <%- include('partial/header') %>
    <div id="main-page" style="text-align: center;">
        <div class="welcome-landlord">
          <h1>Welcome to the Tenant Page</h1>
          <h4><em>"Own Like Never Before"</em></h4>
        </div>
      </div>
    <div class="middle"  >
        <!-- Products Start -->
    <div class="container-fluid pt-5">
        <div class="row px-xl-5 pb-3">
           <!-- copy  -->
            <% if (results.length>0) { 
            results.forEach( function(data){ %>
                <div class="col-lg-3 col-md-6 col-sm-12 pb-1">
                    <div class="card product-item border-0 mb-4">
                        <div class="card-header product-img position-relative overflow-hidden bg-transparent border p-0">
                            <img class="img-fluid w-100" src="img/<%= data.image %>" style="height: 280px !important;" alt="">
                        </div>
                        <div class="card-body border-left border-right text-center p-0 pt-4 pb-3">
                            <h5 class="text-truncate mb-3"><%= data.propertyName %></h5>
                            <h5><%= data.type %></h5>
                            <div class="d-flex justify-content-center">
                                <h6><%= data.lease_start %></h6><h6 class="text-muted ml-2"><del><%= data.lease_end %></del></h6>
                            </div>
                        </div>
                        <div class="card-footer d-flex justify-content-between bg-light border">
                            <a href="" class="btn btn-sm text-dark p-0"><i class="fas fa-thumbs-up text-primary mr-1"></i>Like</a>
                            <a href="" class="btn btn-sm text-dark p-0"><i class="fas fa-thumbs-down text-primary mr-1"></i>Dislike</a>
                            <a href="/details/<%= data.propertyId %>" class="btn btn-sm text-dark p-0"><i class="fas fa-eye text-primary mr-1"></i>Details</a>
                            <a href="/lease/<%= data.propertyId %>" class="btn btn-sm text-dark p-0"><i class="fas fa-home text-primary mr-1"></i>Lease</a>
                        </div>
                    </div>
                </div>
            <%  });
            }else{ %>
             <p style=" color: brown; ">No Property Updated Yet</p>
            <% } %>       
            
        </div>
    </div>
    <!-- Products End -->


    </div>       

    <%- include('partial/footer') %>
</body>

</html>